{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-coverflow-carousel-{{ ai_gen_id }} {
    display: block;
    width: 100%;
    padding: {{ block.settings.padding_top }}px 0 {{ block.settings.padding_bottom }}px;
    background-color: {{ block.settings.background_color }};
    overflow: hidden;
  }

  .ai-coverflow-carousel__container-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    max-width: {{ block.settings.container_width }}px;
    margin: 0 auto;
    padding: 0 20px;
  }

  .ai-coverflow-carousel__heading-{{ ai_gen_id }} {
    text-align: {{ block.settings.heading_alignment }};
    margin-bottom: {{ block.settings.heading_spacing }}px;
    color: {{ block.settings.heading_color }};
    font-size: {{ block.settings.heading_size }}px;
  }

  .ai-coverflow-carousel__wrapper-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    height: {{ block.settings.carousel_height }}px;
    perspective: {{ block.settings.perspective }}px;
    overflow: visible;
  }

  .ai-coverflow-carousel__track-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    justify-content: center;
    height: 100%;
    position: relative;
    transform-style: preserve-3d;
    transition: transform {{ block.settings.transition_speed }}ms ease-in-out;
  }

  .ai-coverflow-carousel__slide-{{ ai_gen_id }} {
    position: absolute;
    width: {{ block.settings.slide_width }}px;
    height: {{ block.settings.slide_height }}px;
    transition: all {{ block.settings.transition_speed }}ms ease-in-out;
    transform-style: preserve-3d;
    cursor: pointer;
  }

  .ai-coverflow-carousel__slide-link-{{ ai_gen_id }} {
    display: block;
    width: 100%;
    height: 100%;
    position: relative;
    overflow: hidden;
    border-radius: {{ block.settings.slide_border_radius }}px;
    box-shadow: {{ block.settings.slide_shadow_x }}px {{ block.settings.slide_shadow_y }}px {{ block.settings.slide_shadow_blur }}px {{ block.settings.slide_shadow_color }};
  }

  .ai-coverflow-carousel__slide-link-{{ ai_gen_id }}:hover {
    box-shadow: {{ block.settings.slide_shadow_x | times: 1.5 }}px {{ block.settings.slide_shadow_y | times: 1.5 }}px {{ block.settings.slide_shadow_blur | times: 1.5 }}px {{ block.settings.slide_shadow_color }};
  }

  .ai-coverflow-carousel__image-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: cover;
    display: block;
  }

  .ai-coverflow-carousel__placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    background-color: #f4f4f4;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-coverflow-carousel__placeholder-{{ ai_gen_id }} svg {
    width: 60%;
    height: 60%;
    opacity: 0.3;
  }

  .ai-coverflow-carousel__nav-{{ ai_gen_id }} {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    width: {{ block.settings.nav_button_size }}px;
    height: {{ block.settings.nav_button_size }}px;
    background-color: {{ block.settings.nav_button_bg }};
    color: {{ block.settings.nav_button_color }};
    border: none;
    border-radius: 50%;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 10;
    transition: all 0.3s ease;
    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.15);
  }

  .ai-coverflow-carousel__nav-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.nav_button_hover_bg }};
    color: {{ block.settings.nav_button_hover_color }};
    transform: translateY(-50%) scale(1.1);
  }

  .ai-coverflow-carousel__nav-{{ ai_gen_id }}:disabled {
    opacity: 0.3;
    cursor: not-allowed;
  }

  .ai-coverflow-carousel__nav-{{ ai_gen_id }}:disabled:hover {
    transform: translateY(-50%) scale(1);
  }

  .ai-coverflow-carousel__nav--prev-{{ ai_gen_id }} {
    left: {{ block.settings.nav_button_offset }}px;
  }

  .ai-coverflow-carousel__nav--next-{{ ai_gen_id }} {
    right: {{ block.settings.nav_button_offset }}px;
  }

  .ai-coverflow-carousel__nav-icon-{{ ai_gen_id }} {
    width: 20px;
    height: 20px;
  }

  @media screen and (max-width: 768px) {
    .ai-coverflow-carousel__wrapper-{{ ai_gen_id }} {
      height: {{ block.settings.carousel_height | times: 0.7 }}px;
    }

    .ai-coverflow-carousel__slide-{{ ai_gen_id }} {
      width: {{ block.settings.slide_width | times: 0.7 }}px;
      height: {{ block.settings.slide_height | times: 0.7 }}px;
    }

    .ai-coverflow-carousel__heading-{{ ai_gen_id }} {
      font-size: {{ block.settings.heading_size | times: 0.7 }}px;
    }

    .ai-coverflow-carousel__nav-{{ ai_gen_id }} {
      width: {{ block.settings.nav_button_size | times: 0.8 }}px;
      height: {{ block.settings.nav_button_size | times: 0.8 }}px;
    }

    .ai-coverflow-carousel__nav--prev-{{ ai_gen_id }} {
      left: 10px;
    }

    .ai-coverflow-carousel__nav--next-{{ ai_gen_id }} {
      right: 10px;
    }
  }
{% endstyle %}

<coverflow-carousel-{{ ai_gen_id }}
  class="ai-coverflow-carousel-{{ ai_gen_id }}"
  {{ block.shopify_attributes }}
  data-rotation="{{ block.settings.rotation_angle }}"
  data-depth="{{ block.settings.depth_offset }}"
  data-scale="{{ block.settings.scale_factor }}"
  data-loop="{{ block.settings.enable_loop }}"
>
  <div class="ai-coverflow-carousel__container-{{ ai_gen_id }}">
    {% if block.settings.heading != blank %}
      <h2 class="ai-coverflow-carousel__heading-{{ ai_gen_id }}">{{ block.settings.heading }}</h2>
    {% endif %}

    <div class="ai-coverflow-carousel__wrapper-{{ ai_gen_id }}">
      <div class="ai-coverflow-carousel__track-{{ ai_gen_id }}">
        {% for i in (1..7) %}
          {% liquid
            assign image_key = 'slide_' | append: i | append: '_image'
            assign link_key = 'slide_' | append: i | append: '_link'
            assign image = block.settings[image_key]
            assign link = block.settings[link_key]
          %}

          <div class="ai-coverflow-carousel__slide-{{ ai_gen_id }}" data-index="{{ forloop.index0 }}">
            {% if link != blank %}
              <a href="{{ link }}" class="ai-coverflow-carousel__slide-link-{{ ai_gen_id }}">
            {% else %}
              <div class="ai-coverflow-carousel__slide-link-{{ ai_gen_id }}">
            {% endif %}
              {% if image %}
                <img
                  src="{{ image | image_url: width: 800 }}"
                  alt="{{ image.alt | escape }}"
                  class="ai-coverflow-carousel__image-{{ ai_gen_id }}"
                  loading="lazy"
                  width="800"
                  height="800"
                >
              {% else %}
                <div class="ai-coverflow-carousel__placeholder-{{ ai_gen_id }}">
                  {{ 'image' | placeholder_svg_tag }}
                </div>
              {% endif %}
            {% if link != blank %}
              </a>
            {% else %}
              </div>
            {% endif %}
          </div>
        {% endfor %}
      </div>

      {% if block.settings.show_navigation %}
        <button
          class="ai-coverflow-carousel__nav-{{ ai_gen_id }} ai-coverflow-carousel__nav--prev-{{ ai_gen_id }}"
          aria-label="Previous slide"
        >
          <svg class="ai-coverflow-carousel__nav-icon-{{ ai_gen_id }}" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <polyline points="15 18 9 12 15 6"></polyline>
          </svg>
        </button>
        <button
          class="ai-coverflow-carousel__nav-{{ ai_gen_id }} ai-coverflow-carousel__nav--next-{{ ai_gen_id }}"
          aria-label="Next slide"
        >
          <svg class="ai-coverflow-carousel__nav-icon-{{ ai_gen_id }}" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <polyline points="9 18 15 12 9 6"></polyline>
          </svg>
        </button>
      {% endif %}
    </div>
  </div>
</coverflow-carousel-{{ ai_gen_id }}>

<script>
  (function() {
    class CoverflowCarousel{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.currentIndex = 0;
        this.slides = [];
        this.totalSlides = 0;
        this.isAnimating = false;
      }

      connectedCallback() {
        this.slides = Array.from(this.querySelectorAll('.ai-coverflow-carousel__slide-{{ ai_gen_id }}'));
        this.totalSlides = this.slides.length;
        this.prevButton = this.querySelector('.ai-coverflow-carousel__nav--prev-{{ ai_gen_id }}');
        this.nextButton = this.querySelector('.ai-coverflow-carousel__nav--next-{{ ai_gen_id }}');
        this.rotationAngle = parseFloat(this.dataset.rotation) || 50;
        this.depthOffset = parseFloat(this.dataset.depth) || 200;
        this.scaleFactor = parseFloat(this.dataset.scale) || 0.8;
        this.enableLoop = this.dataset.loop === 'true';

        this.setupEventListeners();
        this.updateSlides();
      }

      setupEventListeners() {
        if (this.prevButton) {
          this.prevButton.addEventListener('click', () => this.navigate(-1));
        }
        if (this.nextButton) {
          this.nextButton.addEventListener('click', () => this.navigate(1));
        }

        this.slides.forEach((slide, index) => {
          slide.addEventListener('click', (e) => {
            if (index !== this.currentIndex) {
              e.preventDefault();
              const diff = index - this.currentIndex;
              this.navigate(diff);
            }
          });
        });

        let touchStartX = 0;
        let touchEndX = 0;

        this.addEventListener('touchstart', (e) => {
          touchStartX = e.changedTouches[0].screenX;
        });

        this.addEventListener('touchend', (e) => {
          touchEndX = e.changedTouches[0].screenX;
          this.handleSwipe(touchStartX, touchEndX);
        });
      }

      handleSwipe(startX, endX) {
        const diff = startX - endX;
        if (Math.abs(diff) > 50) {
          if (diff > 0) {
            this.navigate(1);
          } else {
            this.navigate(-1);
          }
        }
      }

      navigate(direction) {
        if (this.isAnimating) return;

        const newIndex = this.currentIndex + direction;

        if (!this.enableLoop) {
          if (newIndex < 0 || newIndex >= this.totalSlides) return;
        }

        this.isAnimating = true;
        this.currentIndex = ((newIndex % this.totalSlides) + this.totalSlides) % this.totalSlides;
        this.updateSlides();

        setTimeout(() => {
          this.isAnimating = false;
        }, 500);
      }

      updateSlides() {
        this.slides.forEach((slide, index) => {
          const position = index - this.currentIndex;
          const absPosition = Math.abs(position);

          let translateX = 0;
          let translateZ = 0;
          let rotateY = 0;
          let scale = 1;
          let zIndex = 0;
          let opacity = 1;

          if (position === 0) {
            translateX = 0;
            translateZ = 0;
            rotateY = 0;
            scale = 1;
            zIndex = 10;
            opacity = 1;
          } else {
            const direction = position > 0 ? 1 : -1;
            translateX = direction * (absPosition * 150 + 100);
            translateZ = -absPosition * this.depthOffset;
            rotateY = -direction * this.rotationAngle;
            scale = Math.pow(this.scaleFactor, absPosition);
            zIndex = 10 - absPosition;
            opacity = absPosition > 2 ? 0 : 1;
          }

          slide.style.transform = `translateX(${translateX}px) translateZ(${translateZ}px) rotateY(${rotateY}deg) scale(${scale})`;
          slide.style.zIndex = zIndex;
          slide.style.opacity = opacity;
        });

        if (this.prevButton && this.nextButton && !this.enableLoop) {
          this.prevButton.disabled = this.currentIndex === 0;
          this.nextButton.disabled = this.currentIndex === this.totalSlides - 1;
        }
      }
    }

    customElements.define('coverflow-carousel-{{ ai_gen_id }}', CoverflowCarousel{{ ai_gen_id }});
  })();
</script>

